#include <iostream>
using namespace std;

int main() {
    int dor=0;
    for (int i = 1; i <= 10; i++) {
        if (i % 3 == 0 || i % 5 == 0) {
            cout << "DOR "; dor++;
        } else {
            cout << i << "   ";
        }
    }
    cout<<endl;
        for (int j = 11; j <= 100; j++) {
        if (j % 3 == 0 || j % 5 == 0) {
            cout << "DOR "; dor++;
        } else {
            cout << j << "  ";
        }
        if (j % 10 == 0)
            cout << endl;
        }
        cout<<endl;
        
    cout << endl << "TOTAL DOR YANG MUNCUL SEBANYAK = " << dor << endl;
}
